{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "categorie",
    "pluralName": "categories",
    "displayName": "Categorie",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "required": true
    },
    "name": {
      "type": "string"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "app_posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog-post.blog-post",
      "mappedBy": "category"
    }
  }
}
