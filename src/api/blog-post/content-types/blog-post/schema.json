{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "singularName": "blog-post",
    "pluralName": "blog-posts",
    "displayName": "App Posts",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "post": {
      "type": "richtext",
      "required": true
    },
    "rating": {
      "type": "string",
      "required": true
    },
    "metaTags": {
      "type": "text"
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "publishedOn": {
      "type": "date",
      "required": true
    },
    "size": {
      "type": "string"
    },
    "Applogo": {
      "type": "string"
    },
    "Image01": {
      "type": "string"
    },
    "Image03": {
      "type": "string"
    },
    "Image04": {
      "type": "string"
    },
    "Image05": {
      "type": "string"
    },
    "requirements": {
      "type": "string"
    },
    "publisher": {
      "type": "string"
    },
    "appAgeRating": {
      "type": "enumeration",
      "enum": [
        "Everyone",
        "Teen"
      ],
      "default": "Everyone"
    },
    "downloadLink": {
      "type": "string"
    },
    "downloads": {
      "type": "string"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::blog-category.blog-category",
      "inversedBy": "app_post"
    },
    "latestVersion": {
      "type": "string"
    },
    "isPopular": {
      "type": "boolean"
    },
    "subtitle": {
      "type": "string"
    },
    "isSideCardShow": {
      "type": "boolean"
    }
  }
}
